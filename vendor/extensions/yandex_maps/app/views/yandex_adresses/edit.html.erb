<div class="page-wrapper container">
	<table class="null-table full content-layout">
		<tr>
	      <td class="header" colspan="2">
	        <h1 style="background-image:url(/images/adminsystem/icons/48/content.png);"><%= link_to "Офисы", admin_yandex_maps_path %> / <%= link_to "Адреса", admin_yandex_adresses_path(:map_region_id=>@map_address.map_region.id) %> / Редактируем адрес</h1>
	      </td>
	      <td class="column-right" rowspan="2">
	      </td>
	    </tr>
		<tr>
	      <td class="column-center ">
			<div id="githubstyle">
		        <% form_for :map_address, :url=>admin_yandex_adress_path(@map_address), :html =>{:method=>:put} do |f| %>
					<%= render :partial => "form", :locals => {:f => f }%>
					<%= f.submit "Сохранить", :class=>"minibutton" %>
					<%= link_to "Отменить", admin_yandex_adresses_path(:map_region_id=>@map_address.map_region.id), :class=>"minibutton"%>
				<% end %>
			</div>
	      </td>
	    </tr>
	</table>
</div>