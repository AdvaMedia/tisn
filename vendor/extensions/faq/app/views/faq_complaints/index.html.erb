<div class="page-wrapper container">
	<table class="null-table full content-layout">
		<tr>
	      <td class="header" colspan="2">
	        <h1 style="background-image:url(/images/adminsystem/icons/48/content.png);">Вопросы и ответы / Претензии</h1>
	      </td>
	      <td class="column-right" rowspan="2">
	      </td>
	    </tr>
		<tr>
	      <td class="column-center pagehead">
			<ul class="actions">
				<li>
					<%#= link_to "<span>Задать вопрос</span>", new_admin_faq_question_url, {:class=>"minibutton btn-watch"}%>
				</li>
			</ul>
			<div id="githubstyle">
		        <table class="tree-browser" cellspacing="0" cellpadding="0">
		          <tbody>
					<tr class="header">
						<th>Номер договора</th>
						<th>Претензия</th>
						<th>Дата добавления</th>
						<th>Действия</th>
					</tr>
					<%= render :partial => "list", :locals=>{:collection => @items} %>
		          </tbody>
		        </table>
			</div>
	      </td>
	    </tr>
	</table>
</div>