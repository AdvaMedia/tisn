<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
  <head profile="http://gmpg.org/xfn/11">
    <a2m:pagetitle />
    <meta http-equiv="Content-Type" content="text/html; charset=windows-1251" />
    <meta http-equiv="imagetoolbar" content="no" />
    <%= stylesheet_link_tag %w{cssf-base layout blocks modules}.map{|file| file="/templates/layouts/oknatisn/css/#{file}"} %>
    <%= stylesheet_link_tag %w{ext_blocks mediabox} %>
    <!--[if lt IE 8]><%= stylesheet_link_tag %w{ie}.map{|file| file="/templates/layouts/oknatisn/css/#{file}"} %><![endif]-->
    <%= javascript_include_tag %w{auto_corners_selector css_browser_selector}.map{|file| file="/templates/layouts/oknatisn/js/#{file}"} %>
    <%= javascript_include_tag %w{swfobject mootools-core-1.3 mootools-more-1.3 formcheck/formcheck geophones} %>
  </head>
  <body class="home">
    <div class="page-margin">
      <!--HEADER-->
      <div id="header">
        <!--Logotype-->
        <div class="logo">
          <h1><a class="img-repl" title="Компания Окна ТиСН" href="/">Окна ТиСН</a></h1>
        </div>
        <!--Phones-->
        <div class="phones">
         <a2m:geophones />
        </div>
        <!--Contact us-->
        <a2m:feedback />
        <div class="region block">
          <a2m:region name="contacts_region" title="Регион для поиска" />
        </div>
      </div>
      <!--END HEADER-->
      <!--PROMO-->
      <div id="promo" class="region">
        <a2m:region name="promo_region" title="Промо-регион" />
      </div>
      <!--END PROMO-->
      <!--INDEX CONTENT-->
      <div id="index-content" class="block">
        <div class="columns">
          <div class="w-60">
            <div class="col-content col-first">
              <div class="region">
                <a2m:region name="center_region" title="Центральный регион - 1" />
              </div>
            </div>
          </div>
          <div class="w-40">
            <div class="col-content col-last">
              <a2m:region name="content_region" title="Центральный регион - 2" />
            </div>
          </div>
        </div>
      </div>
      <!--END INDEX CONTENT-->
      <!--FOOTER-->
      <div id="footer">
        <div class="region">
          <a2m:region name="footer_region" title="Подвал сайта" />
        </div>
      </div>
      <!--END FOOTER-->
    </div><!--End Page margin-->
    <a2m:all_phones />
    <script type="text/javascript">CornersInit();</script>
<!-- Yandex.Metrika -->
<script src="//mc.yandex.ru/metrika/watch.js" type="text/javascript"></script>
<div style="display:none;"><script type="text/javascript">
try { var yaCounter1219686 = new Ya.Metrika(1219686); } catch(e){}
</script></div>
<noscript><div style="position:absolute"><img src="//mc.yandex.ru/watch/1219686" alt="" /></div></noscript>
<!-- /Yandex.Metrika -->
  </body>
</html>
